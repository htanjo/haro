{
  "name": "haro",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server --exec ts-node server/index.ts",
    "ssl": "npm-run-all --parallel ssl:*",
    "ssl:dev": "npm run dev",
    "ssl:proxy": "local-ssl-proxy --source 8686 --target 5173",
    "build": "npm-run-all build:*",
    "build:server": "rimraf server-dist && tsc --build server --verbose",
    "build:client": "vite build",
    "start": "cross-env NODE_ENV=production node server-dist/index.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.10.0",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-speech-recognition": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-speech-recognition": "^3.9.6",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "local-ssl-proxy": "^2.0.5",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
